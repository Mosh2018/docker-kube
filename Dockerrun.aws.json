{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "ui",
      "image": "mosh2020/ui",
      "hostname": "ui",
      "essential": false
    },
    {
      "name": "web",
      "image": "mosh2020/web",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "mosh2020/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "links": ["ui", "web"]
    }
  ]
}
